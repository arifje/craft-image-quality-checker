{
  "name": "arjan-brinkman/craft-image-quality-checker",
  "description": "Checks an image after upload on multiple quality criterias like blurriness, resolution, noise, and alerts user.",
  "type": "craft-plugin",
  "version": "1.2.0",
  "require": {
    "php": ">=8.2",
    "craftcms/cms": "^4.0.0|^5.0.0"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "arjanbrinkman\\craftimagequalitychecker\\": "src/"
    }
  },
  "extra": {
    "handle": "_image-quality-checker",
    "name": "Image Quality Checker",
    "developer": "Arjan Brinkman",
    "documentationUrl": "",
    "class": "arjanbrinkman\\craftimagequalitychecker\\ImageQualityChecker"
  },
  "scripts": {
    "check-cs": "ecs check --ansi",
    "fix-cs": "ecs check --ansi --fix",
    "phpstan": "phpstan --memory-limit=1G"
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}
